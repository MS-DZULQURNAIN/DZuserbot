{
  "name": "DZuserbot",
  "description": "DZuserbot project by @MSDZULQRNN",
  "logo": "",
  "keywords": [
    "DZuserbot"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/MS-DZULQURNAIN/DZuserbot",
  "env": {
    "ENVIRONMENT": {
      "description": "GAUSAH DIUBAH",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "API_ID dari my.telegram.org.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "API_HASH dari my.telegram.org.",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "BOT_TOKEN dari @BotFather",
      "required": true,
      "value": ""
    },
    "MUST_JOIN": {
      "description": "Username/id FSUB (jadikan bot sebagai admin)",
      "required": false,
      "value": ""
    }
  }
}
